<Application
    x:Class="ImageTag.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ImageTag"
    Startup="Application_Startup"
    StartupUri="InitWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/Expander.xaml" />
                <ResourceDictionary Source="/Style/Button.xaml" />
                <ResourceDictionary Source="/Style/CheckBox.xaml" />
                <ResourceDictionary Source="/Style/ComboBox.xaml" />
                <ResourceDictionary Source="/Style/ContextMenu.xaml" />
                <ResourceDictionary Source="/Style/GroupBox.xaml" />
                <ResourceDictionary Source="/Style/Label.xaml" />
                <ResourceDictionary Source="/Style/ListView.xaml" />
                <ResourceDictionary Source="/Style/RadioButton.xaml" />
                <ResourceDictionary Source="/Style/Slider.xaml" />
                <ResourceDictionary Source="/Style/TabControl.xaml" />
                <ResourceDictionary Source="/Style/TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
